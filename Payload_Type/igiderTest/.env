# === Core Configuration ===
C2_INTERVAL=60
C2_JITTER=0.3
C2_CHUNK_SIZE=512000
C2_KILLDATE=2024-12-31

# === Encryption Settings ===
C2_AES_PSK=base64_encoded_aes256_key_here
C2_KEY_EXCHANGE=true

# === Dynamic Endpoint Configuration ===
# GET endpoints (JSON encoded list)
C2_GET_URLS='["http://192.168.79.6:17443", "https://cdn.example.org:443"]'
C2_GET_URI="/api/v1/feed"
C2_GET_QUERY_PARAMS='[{"name": "q", "value": "message", "transforms": [{"function": "base64"}, {"function": "random_mixed", "parameters": [8]}]}]'

# POST endpoints (JSON encoded list)
C2_POST_URLS='["http://192.168.79.6:17443", "https://api.backup.com:443"]'
C2_POST_URI="/data/collect"
C2_POST_BODY_TRANSFORMS='[{"function": "base64"}, {"function": "prepend", "parameters": ["X1"]}]'

# === Network Configuration ===
C2_PROXY_HOST=proxy.corp.com
C2_PROXY_PORT=8080
C2_PROXY_USER=proxyuser
C2_PROXY_PASS=proxypass

# === OPSEC Transformations ===
C2_USER_AGENT="Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36"
C2_HEADER_TRANSFORMS='[{"name": "X-Client", "value": "message", "transforms": [{"function": "random_number", "parameters": [5]}]}]'

# === Advanced Settings ===
C2_FAILOVER_TIMEOUT=300
C2_MAX_RETRIES=5